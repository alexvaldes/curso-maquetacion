.Contenido
	background green
	width 100%
	display flex
	flex-direction column
	overflow-y scroll
	&-skill
		background-image url(../img/fondo.png)
		background-size cover
		min-height 10em
		display flex
		&-info
			display flex
			flex-direction column
			justify-content center
			padding-left: 22em;
			background: rgba(161,189,134,0.58);
			background: -moz-linear-gradient(left, rgba(161,189,134,0.58) 0%, rgba(200,242,92,0.58) 53%, rgba(210,255,82,0.7) 66%, rgba(210,255,82,1) 100%);
			background: -webkit-gradient(left top, right top, color-stop(0%, rgba(161,189,134,0.58)), color-stop(53%, rgba(200,242,92,0.58)), color-stop(66%, rgba(210,255,82,0.7)), color-stop(100%, rgba(210,255,82,1)));
			background: -webkit-linear-gradient(left, rgba(161,189,134,0.58) 0%, rgba(200,242,92,0.58) 53%, rgba(210,255,82,0.7) 66%, rgba(210,255,82,1) 100%);
			background: -o-linear-gradient(left, rgba(161,189,134,0.58) 0%, rgba(200,242,92,0.58) 53%, rgba(210,255,82,0.7) 66%, rgba(210,255,82,1) 100%);
			background: -ms-linear-gradient(left, rgba(161,189,134,0.58) 0%, rgba(200,242,92,0.58) 53%, rgba(210,255,82,0.7) 66%, rgba(210,255,82,1) 100%);
			background: linear-gradient(to right, rgba(161,189,134,0.58) 0%, rgba(200,242,92,0.58) 53%, rgba(210,255,82,0.7) 66%, rgba(210,255,82,1) 100%);
